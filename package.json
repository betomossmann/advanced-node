{
  "name": "advanced-node",
  "version": "1.0.0",
  "description": "Professional API with Clean Architecture and TDD",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:w": "npm test -- --watch",
    "test:s": "npm test -- --findRelatedTests",
    "test:co": "npm test -- --coverage",
    "test:api": "npm test -- -c ./jest.integration.config.js ./tests/external/facebook-api.test.ts",
    "prepare": "husky install",
    "check": "npm-check -s -u"
  },
  "keywords": [
    "clean architecture",
    "clean code",
    "tdd",
    "ddd",
    "solid",
    "design patterns",
    "node",
    "typescript",
    "jest",
    "typeorm"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest-mock/express": "^2.0.1",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^20.2.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "eslint": "^8.40.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "lint-staged": "^13.2.2",
    "pg-mem": "^1.9.17",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "typeorm": "0.2.29"
  }
}
